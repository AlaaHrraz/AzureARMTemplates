{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "webAppNamePrefix": {
      "type": "string",
      "minLength": 1
    },
    "farmSkuName": {
      "type": "string",
      "defaultValue": "S1",
      "allowedValues": [
        "F1",
        "D1",
        "B1",
        "B2",
        "B3",
        "S1",
        "S2",
        "S3",
        "P1",
        "P2",
        "P3",
        "P4"
      ],
      "metadata": {
        "description": "Describes plan's pricing tier and capacity. Check details at https://azure.microsoft.com/en-us/pricing/details/app-service/"
      }
    },
    "serverFarmApiVersion": {
      "type": "string",
      "metadata": {
        "description": "Azure API for Server Farm API Version ."
      }
    }

  },
  "variables": {},
  "resources": [
    {
      "name": "[parameters('webAppNamePrefix')]",
      "type": "Microsoft.Web/serverfarms",
      "location": "[resourceGroup().location]",
      "apiVersion": "[parameters('serverFarmApiVersion')]",
      "sku": {
        "name": "[parameters('webAppNamePrefix')]"
      },
      "dependsOn": [],
      "tags": {
        "displayName": "[parameters('webAppNamePrefix')]"
      },
      "properties": {
        "name": "[parameters('webAppNamePrefix')]",
        "numberOfWorkers": 1
      }
    }],
  "outputs": {}
}
